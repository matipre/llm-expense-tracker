# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# Supabase Configuration (get these values from `supabase start` output)
SUPABASE_URL=http://localhost:54321
SUPABASE_SERVICE_KEY=your_service_role_key_from_supabase_start_output

# Database Configuration (get this URL from `supabase start` output)
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:5432/postgres

# Application Configuration
PORT=3002
ENVIRONMENT=development
LOG_LEVEL=INFO

# Queue Configuration
QUEUE_POLL_INTERVAL=5

# Job Factory Configuration
JOB_POLL_INTERVAL=5
JOB_BATCH_SIZE=10
JOB_VISIBILITY_TIMEOUT=30
